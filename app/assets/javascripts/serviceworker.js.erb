self.addEventListener("push", function(event) {
  var body = (event.data && event.data.text());
  var tag = "reviewit";
  var icon = "<%= asset_path 'favicon/android-chrome-144x144.png' %>";

  event.waitUntil(self.registration.showNotification('Reviewit', { body: body, icon: icon, tag: tag }));
});
